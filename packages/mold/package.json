{
	"name": "@produck/mold",
	"version": "0.1.1",
	"description": "For helping to create a final options from a defined specification.",
	"keywords": [
		"options",
		"normalize",
		"utils"
	],
	"author": "ChaosLee <chaos2lee@qq.com>",
	"homepage": "https://github.com/produck/mold/tree/main/packages/mold#readme",
	"license": "MIT",
	"types": "types/index.d.js",
	"files": [
		"dist/mold.cjs",
		"src",
		"types",
		"tsconfig.json"
	],
	"publishConfig": {
		"access": "public"
	},
	"main": "dist/mold.cjs",
	"type": "module",
	"exports": {
		"import": "./src/index.mjs",
		"require": "./dist/mold.cjs"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/produck/mold.git"
	},
	"scripts": {
		"build": "rollup -c script/rollup.config.mjs",
		"lint": "eslint --fix **/*.mjs",
		"test": "mocha \"**/*-spec.mjs\" -t 999999999 ",
		"coverage": "c8 --reporter=lcov npm test",
		"prepublishOnly": "npm run lint && npm test && npm run build"
	},
	"bugs": {
		"url": "https://github.com/produck/mold/issues"
	},
	"devDependencies": {
		"c8": "^7.12.0",
		"eslint": "^8.24.0",
		"mocha": "^10.0.0",
		"rollup": "^2.79.1"
	},
	"gitHead": "3bcaf1961b39839e9faa7d92bd4d550b4bc86d1a"
}
